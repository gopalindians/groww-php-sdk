{
    "name": "gopalindians/groww-php-sdk",
    "description": "PHP SDK for Groww Trading API",
    "type": "library",
    "keywords": ["groww", "trading", "api", "stocks", "sdk", "finance", "investment","algo-trading","stock-market","stock-broker","stock-market-api","stock-market-sdk","stock-market-broker","stock-market-trading","stock-market-investment","stock-market-algo-trading"],
    "homepage": "https://github.com/gopalindians/groww-php-sdk",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Groww\\API\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Groww\\API\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Gopal Sharma",
            "email": "gopalindians@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/gopalindians/groww-php-sdk/issues",
        "source": "https://github.com/gopalindians/groww-php-sdk"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "format": "php-cs-fixer fix --allow-risky=yes"
    }
} 